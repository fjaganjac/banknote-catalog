    "@types/isaac": "0.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.172",
    "@types/node": "^15.0.2",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.28",
    "http-proxy-middleware": "^2.0.0",
    "less": "^4.1.2",
    "node-sass": "^6.0.0"
