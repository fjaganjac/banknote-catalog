{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "ant": "^0.2.0",
    "ant-table-extensions": "^1.1.7",
    "antd": "^4.20.6",
    "awilix": "^4.3.4",
    "base-64": "^1.0.0",
    "crypto-js": "^4.0.0",
    "history": "^5.2.0",
    "interweave": "^12.9.0",
    "isaac": "0.0.5",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "styled-components": "^5.3.5",
    "typescript": "^4.2.4",
    "vite": "^2.6.3",
    "vite-plugin-svgr": "^0.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && rm -rf ../../src/main/resources/static && mv build ../../src/main/resources/static",
    "lint:fix": "eslint src/**/*.js --fix",
    "serve": "vite preview",
    "scss": "node-sass --watch src/ui/assets/scss -o src/ui/assets/css"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@types/base-64": "^1.0.0",
    "@types/crypto-js": "^4.0.1",
