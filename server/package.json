{
  "name": "banknote-catalog",
  "version": "1.0.0",
  "description": "Bankonte catalog",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "npm run watch",
    "migrate": "flyway -c db/flyway_config.js migrate",
    "undo-migration": "flyway -c db/flyway_config.js undo",
    "migration-info": "flyway -c db/flyway_config.js info",
    "clean-db": "flyway -c db/flyway_config.js clean",
    "reset-db": "npm run clean-db && npm run migrate",
    "develop": "ts-node --inspect -r tsconfig-paths/register ./src/main.ts",
    "watch": "tsc -w",
    "build": "tsc"
  },
  "keywords": [
    "banknote",
    "catalog"
  ],
  "author": "PING",
  "license": "ISC",
  "dependencies": {
    "awilix": "^4.0.0",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "^0.0.3",
    "boom": "^7.2.2",
    "datatables.net": "^1.10.19",
    "datatables.net-dt": "^1.10.19",
    "ejs": "^2.6.1",
    "email-templates": "^5.0.2",
    "h2o2": "^8.1.2",
    "handlebars": "^4.0.12",
    "hapi": "^17.7.0",
    "hapi-auth-cookie": "^9.1.0",
    "hapi-auth-jwt2": "^8.1.0",
    "hapi-authorization": "^4.0.0",
    "hapi-dev-errors": "^3.2.0",
    "hapi-ending": "^1.0.0",
    "hapi-swaggered": "^3.0.3",
    "hapi-swaggered-ui": "^3.0.2",
    "inert": "^5.1.2",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "mustache": "^3.0.1",
    "nconf": "^0.10.0",
    "nodemailer": "^4.7.0",
    "promise-mysql": "^3.3.1",
    "raven": "^2.6.4",
    "returnof": "^1.1.1",
    "temp-write": "^3.4.0",
    "uuid": "^3.3.2",
    "vision": "^5.4.3",
    "xlsx": "^0.14.1",
    "yar": "^9.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/boom": "^7.2.1",
    "@types/glob": "^7.1.1",
