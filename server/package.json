    "@types/hapi-auth-jwt2": "^8.0.2",
    "@types/joi": "^14.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.118",
    "@types/mustache": "^0.8.32",
    "@types/mysql": "^2.15.5",
    "@types/nconf": "0.0.37",
    "@types/node": "^10.12.8",
    "@types/nodemailer": "^4.6.5",
    "@types/pug": "^2.0.4",
    "@types/raven": "^2.5.1",
    "@types/temp-write": "^3.3.0",
    "@types/uuid": "^3.4.4",
    "@types/vision": "^5.3.5",
    "@types/yar": "^9.1.0",
    "node-flywaydb": "^0.1.6",
    "nodemon": "^1.18.6",
    "ts-node": "^7.0.1",
